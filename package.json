{
  "name": "nocode-nobug",
  "private": true,
  "description": "No Code, No Bug. A blog with tech articles that might be helpful. Or not! A blog by Alexandre Le Lain.",
  "version": "0.1.0",
  "author": "Alexandre Le Lain <lelain.alexandre@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/alexandre-lelain/nocode-nobug"
  },
  "keywords": [
    "NoCode NoBug",
    "Tech",
    "Articles",
    "Blog"
  ],
  "license": "MIT",
  "scripts": {
    "build": "yarn clean && gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "lint": "yarn eslint --cache src/**/*.{js,tsx,ts}",
    "check-types": "yarn tsc",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "validate": "yarn lint && yarn check-types"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.5",
    "components-extra": "^2.5.0",
    "gatsby": "^2.19.30",
    "gatsby-image": "^2.2.42",
    "gatsby-plugin-alias-imports": "^1.0.5",
    "gatsby-plugin-feed": "^2.3.28",
    "gatsby-plugin-manifest": "^2.2.42",
    "gatsby-plugin-material-ui": "^2.1.6",
    "gatsby-plugin-offline": "^3.0.35",
    "gatsby-plugin-react-helmet": "^3.1.22",
    "gatsby-plugin-sharp": "^2.4.5",
    "gatsby-plugin-sitemap": "^2.2.28",
    "gatsby-plugin-slug": "^1.0.1",
    "gatsby-plugin-styled-components": "^3.1.19",
    "gatsby-plugin-typescript": "^2.2.0",
    "gatsby-remark-copy-linked-files": "^2.1.39",
    "gatsby-remark-reading-time": "^1.1.0",
    "gatsby-source-filesystem": "^2.1.48",
    "gatsby-transformer-remark": "^2.6.53",
    "gatsby-transformer-sharp": "^2.3.16",
    "lodash-es": "^4.17.15",
    "prismjs": "^1.19.0",
    "react": "^16.13.0",
    "react-children-utilities": "^2.0.8",
    "react-dom": "^16.13.0",
    "react-helmet": "^5.2.1",
    "react-markdown": "^4.3.1",
    "styled-components": "^5.0.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "@typescript-eslint/parser": "^2.22.0",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.18.3",
    "husky": "^4.2.3",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "typescript": "^3.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
